{
  "name": "gurpreetatwal/Skeleton-Api",
  "description": "A Slim Framework skeleton for a (almost) REST api that uses JWTs for authentication and Laravel's Eloquent as the ORM",
  "keywords": [
    "microframework",
    "rest",
    "router",
    "psr7",
    "slim",
    "slim3",
    "jwt",
    "eloquent"
  ],
  "homepage": "https://github.com/gurpreetatwal/Skeleton-API",
  "license": "MIT",
  "authors": [
    {
      "name": "Gurpreet Atwal",
      "email": "gatwal@gatwal.com",
      "homepage": "http://gatwal.com/"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "slim/slim": "~3.0",
    "monolog/monolog": "^1.17",
    "illuminate/database": "^5.2.0",
    "palanik/corsslim": "dev-slim3",
    "phpunit/phpunit": "^5.1",
    "tuupola/slim-jwt-auth": "^2.0",
    "siriusphp/validation": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "SkeletonAPI\\": "src"
    }
  }
}
